<div class="container-fluid container-home">
  <div class="col-md-2"></div>
  <div class="col-md-8">
    <h1 class="home">You Are Authorized</h1>
    <%= image_tag current_user.image_url, class: "img-circle img-thumbnail home-image" %>
    <h2 class="home"> <%= current_user.name %> </h2>
    <h2 class="home"> <%= current_user.bio %> </h2>
    <%= link_to "Logout", "/logout", class: "btn btn-lg btn-default btn-home", method: :delete %>
  </div>
  <div class="col-md-2"></div>
</div>
